{
	"type": "light",
	"colors": {
		"foreground": "#000000",
		"focusBorder": "#fafbfc",
		"scrollbar.shadow": "#00000000",
		"editor.background": "#fcfcfc",
		"editor.foreground": "#000000",
		"editor.lineHighlightBackground": "#fffbdd",
		"editorIndentGuide.background": "#eeeeee",
		"editorLineNumber.foreground": "#cccccc",
		"editorBracketMatch.background": "#f1f8ff",
		"editorBracketMatch.border": "#c8e1ff",
		"tab.activeBackground": "#ffffff",
		"tab.inactiveBackground": "#fafbfc",
		"tab.border": "#e1e4e8",
		"tab.activeBorder": "#0000FF",
		"tab.inactiveForeground": "#586069",
		"editorGroupHeader.tabsBackground": "#fafbfc",
		"editorGroupHeader.tabsBorder": "#e1e4e8",
		"panel.background": "#eeeeee",
		"panelTitle.activeForeground": "#000000",
		"statusBar.foreground": "#24292e",
		"statusBar.noFolderForeground": "#24292e",
		"statusBar.background": "#fafbfc",
		"statusBar.noFolderBackground": "#fafbfc",
		"statusBar.border": "#e1e4e8",
		"activityBar.background": "#24292e",
		"activityBar.foreground": "#fafbfc",
		"activityBarBadge.background": "#54a3ff",
		"sideBar.background": "#eeeeee",
		"sideBar.foreground": "#000000",
		"sideBar.border": "#cccccc",
		"sideBarTitle.foreground": "#24292e",
		"sideBarSectionHeader.background": "#f1f2f3",
		"sideBarSectionHeader.foreground": "#24292e",
		"titleBar.activeBackground": "#fafbfc",
		"titleBar.border": "#e1e4e8",
		"notification.background": "#54a3ff",
		"statusBar.debuggingBackground": "#fafbfc",
		"statusBar.debuggingForeground": "#24292e",
		"editor.selectionBackground": "#99C9EF"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#24292eff",
				"background": "#fcfcfcff"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"strong",
				"markup.heading.markdown",
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.italic.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.link.inline.markdown",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#005cc5"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#dd0000"
			}
		},
		{
			"scope": [
				"keyword",
				"entity.name.type.al",
				"constant.boolean.al"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"scope": [
				"entity.name.trigger.al", 
				"entity.trigger.call1"
			],
			"settings": {
				"foreground": "#000080",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "constant.boolean.al",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"scope": [
				"comment", 
				"comment.line.al.restored"
			],
			"settings": {
				"foreground": "#008000"
			}
		}
	]
}